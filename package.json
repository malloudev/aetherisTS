{
  "name": "aetheris",
  "version": "1.0.0",
  "description": "Typescript backend for the aetheris android app",
  "homepage": "https://github.com/malloudev/aetherisTS#readme",
  "scripts": {
    "prepublishOnly": "npm i --no-bin-links && bash ./build.sh",
    "test": "node ./node_modules/ts-node/dist/bin.js ./lib/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malloudev/aetherisTS.git"
  },
  "keywords": [
    "botdesigner",
    "aoijs",
    "discord"
  ],
  "author": "malloudev",
  "license": "APACHE-2.0",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./**/*": {
      "import": "./dist/mjs",
      "require": "./dist/cjs",
      "types": "./dist/types"
    }
  },
  "bugs": {
    "url": "https://github.com/malloudev/aetherisTS/issues"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.12",
    "@types/ws": "^8.18.1",
    "discord-api-types": "^0.38.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@rainfish/ratelimiter": "^1.0.2",
    "assert": "^2.1.0",
    "eventemitter3": "^5.0.1",
    "node-fetch": "^3.3.2",
    "pako": "^2.1.0",
    "tslib": "^2.8.1",
    "ws": "^8.18.2"
  }
}
